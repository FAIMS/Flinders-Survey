        </term>
        <term> {Artefact}
          <description>Description TBD</description>
        </term>
        <term> {Landscape}
          <description>Description TBD</description>
        </term>
        <term> {zOther}
          <description>Description TBD</description>
        </term>
      </lookup>
    </property>
    <property name="Notes" type="freetext">
      <description>Description TBD</description>
    </property>
    <property name="Photo" type="file">
      <description>Description TBD</description>
    </property>
  </ArchaeologicalElement>


